{
  "openapi": "3.0.0",
  "info": {
    "title": "Mydock WEB2 API",
    "description": "Mydock WEB2 API",
    "version": "1.0.0"
  },
  "paths": {
    "/api/producers": {
      "get": {
        "tags": [
          "Producer"
        ],
        "summary": "Get lists of producers to autocomplete.",
        "operationId": "4be8a9bd22530e376eb0e9b433a3de2d",
        "parameters": [
          {
            "name": "filter[term]",
            "in": "query",
            "description": "Search term for autocomplete, minimum length of 3 characters",
            "required": false,
            "schema": {
              "type": "string",
              "minLength": 3
            },
            "example": "pojistovna"
          },
          {
            "name": "filter[hidden]",
            "in": "query",
            "description": "Show hidden products? (Old products)",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 0,
              "enum": [
                0,
                1
              ]
            },
            "example": 0
          },
          {
            "name": "filter[region]",
            "in": "query",
            "description": "Filters by region",
            "required": false,
            "schema": {
              "type": "string"
            },
            "examples": {
              "5": {
                "summary": "In case of filter by one region",
                "value": "5"
              },
              "44,12": {
                "summary": "In case of filter by more regions",
                "value": "44,12"
              }
            }
          },
          {
            "name": "filter[category]",
            "in": "query",
            "description": "Filters by category",
            "required": false,
            "schema": {
              "type": "string"
            },
            "examples": {
              "5": {
                "summary": "In case of filter by one category",
                "value": "5"
              },
              "5,11": {
                "summary": "In case of filter by more categories",
                "value": "5,11"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns array of objects",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ProducerItemDTO"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Not logged user.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExceptionDTO"
                }
              }
            }
          },
          "403": {
            "description": "No rights for this action.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExceptionDTO"
                }
              }
            }
          },
          "500": {
            "description": "Unexpected server error.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExceptionDTO"
                }
              }
            }
          }
        },
        "route": null,
        "routeType": null
      }
    }
  },
  "components": {
    "schemas": {
      "ErrorDetailDTO": {
        "properties": {
          "code": {
            "type": "string",
            "example": "string"
          },
          "message": {
            "type": "string",
            "example": "string"
          }
        },
        "type": "object"
      },
      "ExceptionDTO": {
        "properties": {
          "errors": {
            "type": "object",
            "nullable": true,
            "additionalProperties": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/ErrorDetailDTO"
              }
            }
          },
          "type": {
            "type": "string",
            "example": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "example": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "example": 0,
            "nullable": true
          },
          "detail": {
            "type": "string",
            "example": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "example": "string",
            "nullable": true
          }
        },
        "type": "object"
      },
      "ProducerItemDTO": {
        "properties": {
          "id": {
            "type": "integer",
            "example": 100
          },
          "title": {
            "type": "string",
            "example": "Agra pojišťovna"
          },
          "commisionTransfer": {
            "type": "ElectronicTradeApi\\Enum\\CommisionTransferEnum",
            "enum": [
              "None",
              "TransferWithBreakdown",
              "TransferWithoutBreakdown"
            ],
            "example": "None"
          }
        },
        "type": "object"
      },
      "ProductItemDTO": {
        "properties": {
          "id_product": {
            "type": "integer",
            "example": 1105
          },
          "title": {
            "type": "string",
            "example": "1. fond reverzních hypoték SICAV, a. s."
          },
          "hidden": {
            "description": "Old products (Staré produkty)",
            "type": "boolean",
            "example": false,
            "nullable": true
          }
        },
        "type": "object"
      }
    }
  },
