{
  "name": "rest2ts_repo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "test:watch": "turbo run test:watch",
    "tsc": "turbo run tsc"
  },
  "devDependencies": {
    "@rest2ts/eslint-config": "workspace:*",
    "@rest2ts/typescript-config": "workspace:*",
    "prettier": "3.3.1",
    "turbo": "2.0.3"
  },
  "packageManager": "pnpm@9.1.4",
  "engines": {
    "node": ">=18"
  }
}